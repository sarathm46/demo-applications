<div style="padding: 20px;">
	<center>
		<h3>Settings</h3>
	</center>
	<div class="form-group">
		<label for="nameCheck">Name</label> <input type="checkbox"
			ng-model="savedList.settings.nameCheck" id="nameCheck">
	</div>
	<div class="form-group">
		<label for="phoneNumberCheck">Phone Number</label> <input
			type="checkbox" ng-model="savedList.settings.phoneNumberCheck"
			id="phoneNumberCheck">
	</div>
	<div class="form-group">
		<label for="addressCheck" >Address</label> <input type="checkbox"
			ng-model="savedList.settings.addressCheck" id="addressCheck">
	</div>
</div>